{
    "name": "jedymatt/laravel-sail-env",
    "description": "Configures .env file to match the sail environment variable's requirements.",
    "keywords": [
        "laravel",
        "laravel/sail",
        "sail",
        "env"
    ],
    "support": {
        "issues": "https://github.com/jedymatt/laravel-sail-env/issues",
        "source": "https://github.com/jedymatt/laravel-sail-env"
    },
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jedymatt\\LaravelSailEnv\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jedy Matt Tabasco",
            "email": "jedymatt@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jedymatt\\LaravelSailEnv\\LaravelSailEnvServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/console": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "symfony/yaml": "^6.0",
        "laravel/sail": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "lint": "vendor/bin/pint"
    }
}
